{
  "scripts": {
    "start": "node ./dist/index.js",
    "server": "nodemon ./dist/index.js",
    "dev": "concurrently \"yarn watch \" \"yarn server\"   \"yarn watch-views\"",
    "watch": "tsc -w",
    "build": "tsc && yarn copy-files",
    "copy-files": "copyfiles -u 1 \"src/views/**/*\" dist/",
    "watch-views": "nodemon --watch src/views -e ejs -x \"yarn copy-files\""
  },
  "dependencies": {
    "@prisma/client": "^5.21.1",
    "bullmq": "^5.23.1",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.21.1",
    "nodemailer": "^6.9.16",
    "prisma": "^5.21.1",
    "zod": "^3.23.8"
  },
  "name": "full-stack-web-01",
  "version": "1.0.0",
  "description": "full stack practice 01",
  "type": "module",
  "main": "index.js",
  "repository": "https://github.com/MRMutahir/full-stack-practice-01.git",
  "author": "Muhammad Mutahir",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/ejs": "^3.1.5",
    "@types/express": "^5.0.0",
    "concurrently": "^9.0.1",
    "nodemon": "^3.1.7",
    "typescript": "^5.6.3"
  }
}
